FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          ftVar;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    inlet1           
    {
        type            fixedValue;
        value           uniform 1e-05;
    }

    outlet1          
    {
        type            inletOutlet;
        inletValue      uniform 1e-05;
        value           uniform 1e-05;
    }

    inlet2           
    {
        type            fixedValue;
        value           uniform 1e-05;
    }

    outlet2          
    {
        type            inletOutlet;
        inletValue      uniform 1e-05;
        value           uniform 1e-05;
    }

    frontAndBack    
    {
        type            empty;
    }
}



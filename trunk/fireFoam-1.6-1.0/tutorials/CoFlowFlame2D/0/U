FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    sides           
    {
        type            pressureInletOutletVelocity;
        phi             phi;
        value           uniform (0 0 0);
    }

    ground           
    {
        type            fixedValue;
        value           uniform (0 0 0);
    } 

    inlet          
    {
        type            flowRateInletVelocity;
        flowRate        0.000287848;       
        value           uniform (0 0 0);
    }

    outlet          
    {
        type            inletOutlet;
        value           uniform (0 0 0);
        inletValue      uniform (0 0 0);  
    }

    frontAndBack    
    {
        type            empty;
    }
}



FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet           
    {
        type            calculated;
        value           uniform 101325;
    }

    outlet          
    {
        type            calculated;
        value           uniform 101325;
    }

    sides           
    {
        type            calculated;
        value           uniform 101325;
    }

    ground          
    {
        type            calculated;
        value           uniform 101325;
    }

    frontAndBack    
    {
        type            empty;
    }
}



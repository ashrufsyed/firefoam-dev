{
    wallHot           
    {
        type            fixedValue;
	value		uniform 0;
    }

    up          
    {
        type            fixedValue;
        value           uniform 0;
    }

    down           
    {
        type            fixedValue;
        value           uniform 0;
    }

    cold          
    {
        type            fixedValue;
        value           uniform 0;
    }

    sides1
    {
        type            cyclic;
    }
}


